{
  "name": "@openobserve/browser-rum",
  "version": "0.1.7",
  "license": "Apache-2.0",
  "main": "cjs/entries/main.js",
  "module": "esm/entries/main.js",
  "types": "cjs/entries/main.d.ts",
  "scripts": {
    "build": "run-p build:cjs build:esm build:bundle",
    "build:bundle": "rm -rf bundle && webpack --mode=production",
    "build:cjs": "rm -rf cjs && tsc -p tsconfig.cjs.json && yarn replace-build-env cjs",
    "build:esm": "rm -rf esm && tsc -p tsconfig.esm.json && yarn replace-build-env esm",
    "replace-build-env": "node ../../scripts/build/replace-build-env.js"
  },
  "dependencies": {
    "@openobserve/browser-core": "0.1.7",
    "@openobserve/browser-rum-core": "0.1.7"
  },
  "peerDependencies": {
    "@openobserve/browser-logs": "0.1.7"
  },
  "peerDependenciesMeta": {
    "@openobserve/browser-logs": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openobserve/browser-sdk.git",
    "directory": "packages/rum"
  },
  "devDependencies": {
    "@types/pako": "2.0.0",
    "pako": "2.1.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
